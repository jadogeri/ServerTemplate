{
  "name": "servertemplate",
  "version": "1.0.0",
  "description": "starter template for backend",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --runInBand --detectOpenHandles --colors",
    "unit": "jest --testPathPattern=unit.test.ts$ --colors",
    "integration": "jest --detectOpenHandles --testPathPattern=integration.test.ts$ --colors --watchAll --no-cache --runInBand --verbose",
    "e2e": "cross-env NODE_ENV=test jest --testPathPattern=e2e.test.ts$ ---detectOpenHandles --forceExit",
    "start": "node index.ts",
    "prod": "nodemon index.ts",
    "dev": "ts-node-dev index.ts",
    "watch": "nodemon --exec ts-node -- ./index.ts",
    "mmd": "nodemon --exec ts-node -- ./tests/index.ts",
    "v2": "nodemon --exec npx ts-node ./index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jadogeri/ServerTemplate.git"
  },
  "author": "Joseph Adogeri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jadogeri/ServerTemplate/issues"
  },
  "homepage": "https://github.com/jadogeri/ServerTemplate#readme",
  "dependencies": {
    "@point-hub/nodemailer-inlinecss": "^1.0.2",
    "@types/bluebird-global": "^3.5.18",
    "@types/express-handlebars": "^5.3.1",
    "@types/mongoose": "^5.11.96",
    "@types/nanoid-dictionary": "^4.2.3",
    "@types/supertest": "^6.0.2",
    "bcrypt": "^5.1.1",
    "bluebird": "^3.7.2",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "dotenv": "^16.6.1",
    "email-templates": "^2.7.1",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "handlebars": "^4.7.8",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-check-expiry": "^1.0.10",
    "jwt-decode": "^4.0.0",
    "mockingoose": "^2.13.1",
    "mongodb-memory-server": "^10.1.3",
    "mongoose": "^8.9.5",
    "nanoid": "^5.0.9",
    "nanoid-dictionary": "^4.3.0",
    "node-localstorage": "^3.0.5",
    "nodemailer": "^6.9.14",
    "nodemailer-juice": "^1.0.1",
    "nodemon": "^3.1.9",
    "path": "^0.12.7",
    "supertest": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "twilio": "^5.4.4",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bluebird": "^3.5.42",
    "@types/email-templates": "^10.0.4",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mockingoose": "^2.13.0",
    "@types/node-localstorage": "^1.3.3",
    "@types/nodemailer": "^6.4.17",
    "mongodb": "^6.20.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2"
  }
}
